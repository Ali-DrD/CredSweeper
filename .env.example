# CredSweeper Configuration
# Copy this file to .env and fill in your API keys

# HaveIBeenPwned API Key (get from https://haveibeenpwned.com/API/Key)
HIBP_API_KEY=your_hibp_api_key_here

# Google Custom Search API (get from https://developers.google.com/custom-search/v1/introduction)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# Logging level (debug, info, warn, error)
LOG_LEVEL=info

# Web interface port
PORT=3000

# Rate limiting settings
RATE_LIMIT_DELAY=1000
HIBP_RATE_LIMIT=1600

# Browser settings for LinkedIn extraction
BROWSER_HEADLESS=true
BROWSER_TIMEOUT=30000